COMMON_WARNINGS := \
	-Wno-int-conversion \
	-Wno-incompatible-pointer-types \
	-Wno-format \
	-Wno-overflow \

CLANG_WARNINGS := \
	-Wno-incompatible-library-redeclaration \
	-Wno-uninitialized \
	-Wno-sometimes-uninitialized \
	-Wno-missing-prototype-for-cc \
	-Wno-bitfield-constant-conversion \

